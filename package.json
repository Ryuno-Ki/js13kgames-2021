{
  "name": "js13kgames-2021",
  "version": "0.1.0",
  "description": "The topic in 2021 is SPACE",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "check": "node ./scripts/zip-and-check.cjs",
    "commit-msg": "commitlint -e",
    "copy:css": "cleancss ./src/css/* -o ./dist/main.css",
    "coverage": "nyc npm run test",
    "deploy": "gh-pages -d ./dist -r git@github.com:Ryuno-Ki/js13kgames-2021.git",
    "docs": "jsdoc -c ./.jsdoc.conf.json -d ./docs -P ./package.json -R README.md -r ./src",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "eslint ./src",
    "lint:md": "markdownlint --ignore ./node_modules **/*.md",
    "postbuild": "npm run copy:css && npm run check",
    "postpost-process": "rm ./dist/*.njk ./dist/*.css ./dist/*.js",
    "posttest": "npm run tsd",
    "post-process": "node -e \"var n=require('nunjucks');console.log(n.render('./dist/index.njk'))\" > ./dist/index.html",
    "prebuild": "npm run docs",
    "precheck": "npm run post-process",
    "predeploy": "npm run build",
    "prepare": "husky install",
    "prepost-process": "cp ./src/index.njk ./dist",
    "prestart": "npm run build",
    "pre-commit": "npm run types && npm run lint && npm run test",
    "start": "http-server -a 0.0.0.0 -p 8080 ./dist",
    "test": "mocha",
    "tsc": "tsc",
    "tsd": "tsd",
    "types": "tsc --noEmit false --emitDeclarationOnly true"
  },
  "repository": {
    "type": "git",
    "url": "ryunoki@jaenis.ch:ryuno-ki/js13kgames-2021.git"
  },
  "type": "module",
  "engines": {
    "node": "16.0.0",
    "npm": "7.10.0"
  },
  "keywords": [],
  "author": "Andr√© Jaenisch",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@rollup/plugin-buble": "0.21.3",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "chai": "4.3.4",
    "clean-css-cli": "5.3.3",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.24.1",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-promise": "5.1.0",
    "gh-pages": "3.2.3",
    "html5-boilerplate": "8.0.0",
    "http-server": "13.0.1",
    "husky": "7.0.1",
    "jsdoc": "3.6.7",
    "jsdoc-mermaid": "1.0.0",
    "jsdoc-tsimport-plugin": "1.0.5",
    "markdownlint-cli": "0.28.1",
    "mocha": "9.1.0",
    "nunjucks": "3.2.3",
    "nyc": "15.1.0",
    "rollup": "2.56.2",
    "rollup-plugin-license": "2.5.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-visualizer": "5.5.2",
    "sinon": "11.1.2",
    "sinon-chai": "3.7.0",
    "standard": "16.0.3",
    "tap": "15.0.9",
    "tsd": "0.17.0",
    "typescript": "4.3.5",
    "zip-dir": "2.0.0"
  }
}
